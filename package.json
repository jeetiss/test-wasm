    "canvas-free-drawing": "^2.1.0",
    "next": "10.1.2",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "assemblyscript": "^0.18.21",
    "file-loader": "^6.2.0",
    "nodemon": "^2.0.7"
  }
}
